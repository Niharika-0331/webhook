pipeline{
    agent any
    stages {
        stage('Git Checkout') {
            steps {
          echo 'Git checkout'
}
}
 stage('build') {
            steps {
          echo 'building the binary'
          echo 'building the binaries'
}
}
 stage('deploy') {
            steps {
          echo 'deploying the binary'
}
}
}
}
